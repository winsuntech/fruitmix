{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node build/app.js",
    "build": "babel src --out-dir build",
    "devel": "PORT=3721 nodemon --exec babel-node src/main.js",
    "devel-family": "PORT=3721 nodemon --ignore 'family/*' --exec babel-node src/main.js --family",
    "fruit-unit": "NODE_PATH=. mocha --compilers js:babel-core/register --recursive --sort test/fruitmix/unit",
    "fruit-algo": "NODE_PATH=. NODE_ENV=test mocha --compilers js:babel-core/register --recursive --sort test/fruitmix/algo",
    "fruit-agent": "NODE_PATH=. NODE_ENV=test mocha --compilers js:babel-core/register --recursive --sort test/fruitmix/agent",
    "mocha": "NODE_PATH=. NODE_ENV=test mocha --compilers js:babel-core/register --recursive --sort",
    "lint": "node node_modules/eslint/bin/eslint.js src",
    "linttest": "node node_modules/eslint/bin/eslint.js test",
    "lintagent": "node node_modules/eslint/bin/eslint.js test_agent"
  },
